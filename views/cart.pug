	body.animsition
		// Header
		header.header1
			// Header desktop
			.container-menu-header
				.wrap_header
					// Logo
					a.logo(href='/')
						img(src='http://www.jobthaiweb.com/company/picture/bnk48office_logo.gif?d=0', alt='IMG-LOGO')
					// Header Icon
					.header-icons
						.header-wrapicon2.m-l-30
							img.header-icon1.js-show-header-dropdown(src='images/icons/icon-header-01.png', alt='ICON')
							// Header cart noti
							.header-cart.header-dropdown
								ul.header-cart-wrapitem
								.header-cart-buttons
									.header-cart-wrapbtn
										// Button
										a.flex-c-m.size1.bg1.bo-rad-20.hov1.s-text1.trans-0-4(href='/')
											| Home
									.header-cart-wrapbtn
										// Button
										if status 
											a.flex-c-m.size1.bg1.bo-rad-20.hov1.s-text1.trans-0-4(href='logout')
												| Log out
										else
											a.flex-c-m.size1.bg1.bo-rad-20.hov1.s-text1.trans-0-4(href='auth/github')
												| Log in
			// Header Mobile
			.wrap_header_mobile
				// Logo moblie
				a.logo-mobile(href='/')
					img(src='images/icons/logo.png', alt='IMG-LOGO')
				// Button show menu
				.btn-show-menu
					// Header Icon mobile
					.header-icons-mobile
						a.header-wrapicon1.dis-block(href='#')
							img.header-icon1(src='images/icons/icon-header-01.png', alt='ICON')
						span.linedivide2
						
					.btn-show-menu-mobile.hamburger.hamburger--squeeze
						span.hamburger-box
							span.hamburger-inner
			// Menu Mobile
			.wrap-side-menu
				nav.side-menu
					ul.main-menu
						li.item-topbar-mobile.p-l-20.p-t-8.p-b-8
							span.topbar-child1
								| Free shipping for standard order over $100
						li.item-topbar-mobile.p-l-20.p-t-8.p-b-8
							.topbar-child2-mobile
								span.topbar-email
									| fashe@example.com
								.topbar-language.rs1-select2
									select.selection-1(name='time')
										option USD
										option EUR
						li.item-topbar-mobile.p-l-10
							.topbar-social-mobile
								a.topbar-social-item.fa.fa-facebook(href='#')
								a.topbar-social-item.fa.fa-instagram(href='#')
								a.topbar-social-item.fa.fa-pinterest-p(href='#')
								a.topbar-social-item.fa.fa-snapchat-ghost(href='#')
								a.topbar-social-item.fa.fa-youtube-play(href='#')
		// Title Page
		section.bg-title-page.p-t-40.p-b-50.flex-col-c-m(style='background-image: url(https://pbs.twimg.com/media/DNtcIx7VAAINw_Y.jpg);')
			h2.l-text2.t-center
				| Cart
		// Cart
		section.cart.bgwhite.p-t-70.p-s-70
			.container
				// Cart item
				.container-table-cart.pos-relative
					.wrap-table-shopping-cart.bgwhite
						table.table-shopping-cart
							tr.table-head
								th.column-1
								th.column-2 Product
								th.column-3 Price
								th.column-4.p-l-30 Quantity
								th.column-5 Total
								th.column-6 Delete
							each product in products
								tr.table-row(id="cart")
									td.column-1
										.cart-img-product.b-rad-4.o-f-hidden
											img(src= product.imgURL, alt='IMG-PRODUCT')
									td.column-2= product.name
									td.column-3= "$"+product.price
									td.column-4
										.flex-w.bo5.of-hidden.w-size17
											button.btn-num-product-down.color1.flex-c-m.size7.bg8.eff2
												i.fs-12.fa.fa-minus(aria-hidden='true')
											input.size8.m-text18.t-center.num-product.a(type='text', name= product.name , value=product.quantity)
											button.btn-num-product-up.color1.flex-c-m.size7.bg8.eff2
												i.fs-12.fa.fa-plus(aria-hidden='true')
									td.column-5= "$"+product.price
									td.column-6
										.flex-w.flex-m.w-full-sm
										.size6
											a.flex-c-m.sizefull.color1.bo-rad-23.hov1.s-text1.trans-0-4(href='cart')
												| x
				.flex-w.flex-sb-m.p-t-25.p-b-25.bo8.p-l-35.p-r-60.p-lr-15-sm
					.flex-w.flex-m.w-full-sm
					.size10.trans-0-4.m-t-10.m-b-10
						// Button
						script(src='https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js')
						script.
							var newVal = $('input[name="num-product1"]').val();
							$(document).ready(function() {
								$('#btn').click(function(){
									var a = document.getElementsByClassName("a");
									var b = [];
									console.log(a.length)
									for(let i = 0; i < a.length; i++){
										b.push(a[i].value);
									}
									console.log(b);
								}); 
							});
						a.flex-c-m.sizefull.bg1.bo-rad-23.hov1.s-text1.trans-0-4(id='btn')
							= "Update Cart"
				
